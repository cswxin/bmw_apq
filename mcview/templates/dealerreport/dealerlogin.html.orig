<script type="text/javascript">
	
	       var trs = "";
           var dealer_infos = "";
           dealer_infos ='{{dealer_login_info}}';
           dealer_info = dealer_infos.replace(/(u&#39;)|(&#39;)|(datetime.datetime\()/g,'"');  
           dealer_info = dealer_info.replace(/(\()/g,'[');
